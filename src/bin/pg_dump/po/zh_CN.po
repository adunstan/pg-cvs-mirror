msgid "  -N, --orig-order         restore in original dump order\n"
msgstr "  -N, --orig-order         以原始的转储顺序恢复\n"

#: pg_restore.c:386
msgid "  -o, --oid-order          restore in OID order\n"
msgstr "  -o, --oid-order          以 OID 顺序恢复\n"

